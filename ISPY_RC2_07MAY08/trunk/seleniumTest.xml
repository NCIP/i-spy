<project name="ispy-test" default="selenese" basedir=".">
	<property file="build.properties" />
	<property file="test.properties" />	

	<target name="selenese"/>
	
	<taskdef resource="selenium-ant.properties">
	    <classpath>
	        <pathelement location="${source.nondeploy.lib.dir}/selenium-server.jar"/>
	    </classpath>
	</taskdef>
	
	<selenese
	    suite="test\selenium\ISPYTestSuite.html"
	    browser="${browser}"
	    port="${server_port}"
	    results="${result_file}"
	    multiWindow="false"
	    timeoutInSeconds="90"
	    startURL="${test_url}"/> 
	
	<selenese
		    suite="test\selenium\ISPYtoNCIATestSuite.html"
		    browser="${browser}"
		    port="${server_port}"
		    results="${result_file}"
		    multiWindow="false"
		    timeoutInSeconds="90"
		    startURL="${test_url2}"/>   
		    
</project>