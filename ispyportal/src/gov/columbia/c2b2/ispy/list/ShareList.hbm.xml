<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE hibernate-mapping PUBLIC 
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="gov.columbia.c2b2.ispy.list" >
	


    	
	<class name="ShareList" table="LIST_SHARE" lazy="true">

		<id name="shareId" type="java.lang.Long" column="SHARE_ID" >
			<generator class="increment" />
		</id>
		
		<property name="listId"
                 type="java.lang.Long"
                 column="LIST_ID"
                 insert="true"
                 update="true" />
        
 		<property name="grId"
                 type="java.lang.Long"
                 column="GR_ID"
                 insert="true"
                 update="true" />
             
        <property name="updateTime"
                 type="timestamp"
                 column="UPDATE_DATE"
                 insert="true"
                 update="true" /> 
