<!DOCTYPE hibernate-mapping PUBLIC 
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="gov.columbia.c2b2.ispy.web.struts.form" >
	


    	
	<class name="UserInfo" table="CSM_USER" lazy="true">

		<id name="userId" type="java.lang.Long" column="USER_ID" >
			<generator class="assigned" />
		</id>
		
 
        <property name="loginName"
                 type="java.lang.String"
                 column="LOGIN_NAME"
                 insert="false"
                 update="false" /> 
        
 
        <property name="firstName"
                 type="java.lang.String"
                 column="FIRST_NAME"
                 insert="true"
                 update="true" /> 
           
		<property name="lastName"
                 type="java.lang.String"
                 column="LAST_NAME"
                 insert="true"
                 update="true" />
        
        <property name="organization"
                 type="java.lang.String"
                 column="ORGANIZATION"
                 insert="true"
                 update="true" />

             
        <property name="department"
                 type="java.lang.String"
                 column="DEPARTMENT"
                 insert="true"
                 update="true" /> 
                
		<property name="title"
                 type="java.lang.String"
                 column="TITLE"
                 insert="true"
                 update="true" />
        
        <property name="phoneNum"
                 type="java.lang.String"
                 column="PHONE_NUMBER"
                 insert="true"
                 update="true" />

             
        <property name="emailID"
                 type="java.lang.String"
                 column="EMAIL_ID"
                 insert="true"
                 update="true" /> 
 <!--                 
         <one-to-one name="groupMembers"
         			class="gov.columbia.c2b2.ispy.web.struts.form.GroupMembers"
         			property-ref="userInfo"
         ></one-to-one> 
 -->               
                          
		</class>
</hibernate-mapping>